{
  "root": ".",
  "clean_urls": false,
  "routes": {
    "/**": "index.html"
  },
  "headers": [
    {
      "source": "**/*.css",
      "headers": [
        { "key": "Content-Type", "value": "text/css; charset=utf-8" },
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    },
    {
      "source": "**/*.js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript; charset=utf-8" },
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    }
  ]
}